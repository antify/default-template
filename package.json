{
  "name": "@antify/default-template",
  "version": "0.0.20",
  "type": "module",
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxi prepare playground",
    "release": "standard-version && git push --follow-tags && pnpm publish --access public",
    "lint": "eslint . --fix"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "style": "./dist/index.css",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@antify/ui": "4.1.24",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@nuxt/kit": "3.16.2",
    "@vueuse/core": "^13.0.0",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.7",
    "@eslint/js": "^9.16.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@nuxt/eslint-config": "^0.4.0",
    "@nuxt/eslint-plugin": "^0.7.6",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/storybook": "8.3.5",
    "@storybook/addon-essentials": "8.6.15",
    "@storybook/vue3": "8.6.15",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@tailwindcss/vite": "4.0.7",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "nuxt": "3.16.2",
    "ofetch": "^1.3.3",
    "standard-version": "^9.5.0",
    "storybook": "8.6.15",
    "storybook-vue3-router": "^5.0.0",
    "tailwindcss": "4.0.7",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.16.0",
    "unbuild": "^2.0.0"
  }
}
